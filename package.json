{
  "name": "gatsby-typescript-tailwind",
  "private": false,
  "description": "A gatsby starter kit using TypeScript and Tailwind CSS",
  "version": "0.1.247",
  "author": "Florentin Eckl <ecklf@icloud.com>",
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@react-keycloak/web": "^3.4.0",
    "@types/": "rafaremo/gatsby-theme-shopify-starter",
    "@types/react-netlify-form": "^2.1.0",
    "framer-motion": "^3.6.2",
    "gatsby-plugin-layout": "^1.10.0",
    "gatsby-plugin-netlify-cms": "^5.5.0",
    "gatsby-plugin-sass": "^4.5.0",
    "gatsby-source-graphql": "^3.0.0",
    "gatsby-source-shopify": "^3.10.0",
    "gatsby-source-shopify-storefront": "^1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "keycloak-js": "^12.0.2",
    "libarchive.js": "^1.3.0",
    "lodash": "^4.17.21",
    "netlify-cms-app": "^2.15.6",
    "nice-react-ticker": "^1.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-icons": "^4.2.0",
    "react-intersection-observer": "^8.32.0",
    "react-marquee-slider": "^1.1.2",
    "react-page-visibility": "^6.4.0",
    "react-parallax": "^3.2.0",
    "react-springy-parallax": "^1.3.0",
    "react-ticker": "^1.2.2",
    "react-toast-notifications": "^2.4.4",
    "react-waypoint": "^9.0.3",
    "sass": "^1.34.0",
    "shopify-buy": "^2.11.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "autoprefixer": "10.0.2",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.14.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "gatsby": "^2.27.0",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-manifest": "2.7.0",
    "gatsby-plugin-offline": "3.5.0",
    "gatsby-plugin-postcss": "3.2.0",
    "gatsby-plugin-purgecss": "5.0.0",
    "gatsby-plugin-react-helmet": "3.5.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-typescript": "2.7.0",
    "gatsby-source-filesystem": "^3.5.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "postcss": "^8.1.9",
    "prettier": "2.2.0",
    "tailwindcss": "^2.0.1",
    "typescript": "4.1.2"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "tailwind",
    "tailwind css",
    "starter"
  ],
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/impulse/gatsby-typescript-tailwind"
  },
  "bugs": {
    "url": "https://github.com/impulse/gatsby-typescript-tailwind/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
